<h1><%= order.phone %></h1><br>
	<% @orders.each do |order| %>
  				
    <h4><a href="/orders/<%= order.id %>"><%= order.lastname %></a> at <%= order.venue %> Table <%= order.table %></h4>
    <h3><%= order.drinks %></h3>
    <p><%= pretty_date(order.created_at) %> from <%= order.phone %></p>
    <br>
  <% end %>