<div id='content'>
	<h3>Austin's Orders</h1><hr>
		<% @orders.each do |order| %>
  				
    	<h5><a href="/venue/<%= order.venue %>"><%= order.venue %></a> at <a href="/venue/<%= order.venue %>/table/<%= order.table %>">Table <%= order.table %></a></h4>
    	<h4><a href="/orders/<%= order.id %>"><%= order.drinks %></a></h3>
    	<p><%= pretty_date(order.created_at) %> from <%= order.firstname %></a></p>
    	<hr>
  	<% end %>
		<script>
      $(document).ready(function() {
      	setTimeout(function() {
        	$('#content').load('/');}, 10000);
      });
    </script>
</div>
